{
  "semi": true,
  "trailingComma": "es5",
  "singleQuote": false,
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "bracketSpacing": true,
  "bracketSameLine": false,
  "arrowParens": "avoid",
  "endOfLine": "lf",
  "quoteProps": "as-needed",
  "jsxSingleQuote": false,
  "plugins": ["prettier-plugin-organize-imports"],
  "overrides": [
    {
      "files": "**/*.{test,spec}.{js,jsx,ts,tsx}",
      "options": {
        "printWidth": 120
      }
    }
  ]
}