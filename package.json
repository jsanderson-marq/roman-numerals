{
  "name": "kata-starter",
  "version": "0.1.0",
  "description": "Starter project for katas with basic Marq linting and testing",
  "private": true,
  "packageManager": "yarn@4.10.3",
  "scripts": {
    "preinstall": "npx only-allow yarn",
    "build": "tsc -p tsconfig.json",
    "lint": "eslint . --ext .js,.jsx,.mjs,.cjs --max-warnings=0",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,css,scss,yaml,yml}\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.3",
    "@types/node": "^24.10.4",
    "@vitest/ui": "4.0.16",
    "dotenv": "17.2.3",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "prettier": "3.7.4",
    "prettier-plugin-organize-imports": "4.3.0",
    "typescript": "5.9.3",
    "vitest": "4.0.16"
  }
}
