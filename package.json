{
  "name": "kata-starter",
  "version": "0.1.0",
  "description": "Starter project for katas with basic Marq linting and testing",
  "private": true,
  "packageManager": "yarn@4.10.3",
  "scripts": {
    "preinstall": "npx only-allow yarn",
    "build": "tsc -p tsconfig.json",
    "lint": "eslint . --ext .js,.jsx,.mjs,.cjs --max-warnings=0",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,css,scss,yaml,yml}\"",
    "test": "jest",
    "test:watch": "yarn test --watch"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.3",
    "@types/jest": "30.0.0",
    "@types/node": "^24.6.1",
    "dotenv": "17.2.3",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "jest": "30.2.0",
    "jest-junit": "16.0.0",
    "prettier": "3.7.4",
    "prettier-plugin-organize-imports": "4.3.0",
    "ts-jest": "29.4.6",
    "typescript": "5.9.3"
  }
}
